@model AutoparkWeb.Models.Entity.SpareParts

@{
    ViewData["Title"] = "New detail";
}
<h2>@ViewData["Title"]</h2>

<form asp-action="CreateSparePart" asp-controller="SpareParts">
    <div>
        <div class="form-group">
            <label asp-for="Name"></label>
            <input type="text" asp-for="Name" class="form-control form-control-sm" />
        </div>

        <div class="form-group">
            <input type="submit" value="Save" class="btn btn-outline-info" />
        </div>
    </div>
</form>

<div>
    <a class="btn btn-outline-info" role="button" asp-controller="SpareParts" asp-action="ViewSpareParts">Back to list of SpareParts</a>
</div>
